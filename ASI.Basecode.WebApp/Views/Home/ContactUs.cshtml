@using static ASI.Basecode.Resources.Views.Screen
@using static ASI.Basecode.Resources.Views.Button
@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<!-- contact section start -->
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="contact_taital">@ContactUs</h1>
            <p class="contact_text">@ReachOut</p>
        </div>
    </div>
    <div class="contact_section_2">
        <div class="row">
            <div class="col-md-12 padding15">
                <form action="">
                    <div class="mail_section_1 ">
                        <input type="text" class="mail_text" placeholder="Name" name="Name">
                        <input type="text" class="mail_text" placeholder="Phone Number" name="Phone Number">
                        <input type="text" class="mail_text" placeholder="Email" name="Email">
                        <textarea class="massage-bt" placeholder="Massage" rows="5" id="comment" name="Massage"></textarea>
                        <div class="send_bt"><a href="#">@SendBtn</a></div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<div class="map_main">
    <div class="map-responsive">
        <iframe src="https://www.google.com/maps/embed/v1/place?key=AIzaSyA0s1a7phLN0iaD6-UE7m4qP-z21pH0eSc&amp;q=UCLM+Mandaue" width="600" height="300" frameborder="0" style="border:0; width: 100%;" allowfullscreen=""></iframe>
    </div>
</div>
<!-- contact section end -->
